{
  "name": "Mojo HelpDesk Extension by Collective Bias",
  "version": "3.5",
  "manifest_version": 2,
  "description": "Unofficial extension for Mojo HelpDesk that adds additional functionallity for tickets received in Gmail.",
  "homepage_url": "http://collectivebias.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options/options.html",
  "web_accessible_resources": ["components/Portlet/stoplight.png","icons/icon19.png","icons/optimus.gif","icons/default_user.png"],
  "permissions": [
    "storage",
    "https://mail.google.com/*",
    "https://mail.collectivebias.com/*",
    "https://mysupport.mojohelpdesk.com/*",
    "https://*.mojohelpdesk.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://mail.collectivebias.com/*",
        "https://*.mojohelpdesk.com/*"
      ],
      "css": [
        "components/Portlet/Portlet.css",
        "components/forms/forms.css"
      ],
      "js": [
        "lib/jquery-2.1.4.min.js",
        "lib/react-0.14.5/react.js",
        "lib/react-0.14.5/react-dom.js",
        "lib/classnames.js",
        "lib/Shared.js",
        "components/Modal.js",
        "components/Portlet/Portlet.js",
        "services/API_Connector.js",
        "components/forms/Ticket_Private_Message_Form.js",
        "components/forms/Ticket_Update_Form.js",
        "components/forms/Ticket_Messages_Form.js",
        "components/Gmail_Sidebar.js",
        "lib/CB_Mojo_Extension.js",
        "index.js"
      ],
      "run_at": "document_end"
    }
  ]
}